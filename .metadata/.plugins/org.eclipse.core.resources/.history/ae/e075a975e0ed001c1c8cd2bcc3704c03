CREATE TABLE tbl_qna(
qno NUMBER,
userid VARCHAR2(300),
qTitle VARCHAR2(300),
qContent VARCHAR2(500),
qWriteDay DATE DEFAULT SYSDATE,
qUpdatedate DATE DEFAULT SYSDATE,
CONSTRAINT tbl_qna PRIMARY KEY (qno)
)

SELECT * FROM tbl_qna

CREATE SEQUENCE seq_qna_qno

CREATE TABLE tbl_uploadqna(
 qno NUMBER,
 filename VARCHAR2(300) NOT NULL,
 CONSTRAINT fk_qna_qno FOREIGN KEY(qno) REFERENCES tbl_qna(qno)
 ON DELETE CASCADE
)
