<mapper namespace="kr.co.coffeemall.qnaupload">

	<insert id="insert"> 
		INSERT INTO tbl_qna_upload(filename, qno) VALUES(#{filename}, #{qno})
	</insert>
	
	<select id="getAllUpload" resultType="string">
		SELECT * FROM tbl_qna_upload WHERE qno = #{qno}
	</select>
</mapper>